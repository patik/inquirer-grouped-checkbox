{
    "name": "inquirer-grouped-checkbox",
    "version": "0.5.1",
    "description": "A searchable, grouped checkbox prompt for Inquirer.js",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "demo": "tsx demo/index.ts",
        "test": "vitest",
        "test:once": "vitest run",
        "typecheck": "tsc --noEmit",
        "lint": "pnpm run typecheck && eslint . --fix && pnpm run format",
        "format": "prettier . --list-different --write",
        "check-format": "prettier . --list-different",
        "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
        "ci": "pnpm build && pnpm check-exports && pnpm test:once",
        "prepublishOnly": "pnpm build"
    },
    "keywords": [
        "inquirer",
        "checkbox",
        "grouped",
        "prompt",
        "cli",
        "select",
        "list",
        "categorized"
    ],
    "author": "Craig Patik",
    "repository": {
        "type": "git",
        "url": "https://github.com/patik/inquirer-grouped-checkbox.git"
    },
    "license": "MIT",
    "packageManager": "pnpm@10.24.0",
    "dependencies": {
        "@inquirer/core": "^11.1.0",
        "@inquirer/figures": "^2.0.2"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.18.2",
        "@eslint/js": "^9.39.2",
        "@inquirer/testing": "^3.0.3",
        "@inquirer/type": "^4.0.2",
        "@types/node": "^25.0.3",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.7.4",
        "tsup": "^8.0.0",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.50.1",
        "vitest": "^4.0.16"
    }
}
